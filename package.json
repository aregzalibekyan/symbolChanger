{
  "name": "namechanger-electron",
  "version": "1.0.0",
  "description": "An app for name changer in folders, more info in github",
  "main": "main.js",
  "scripts": {
    "run": "electron .",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish=always"
  },
  "keywords": [
    "aregzalibekyan"
  ],
  "author": "aregzalibekyan",
  "license": "ISC",
  "devDependencies": {
    "electron": "^29.1.5",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "ejs-electron": "^3.0.0",
    "express": "^4.19.2"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "NameChanger",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "zip"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
